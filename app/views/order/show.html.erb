<h1>Order</h1>
<div class="col-md-6 col-md-offset-3">
	<table class="table">
		<thead>
			<tr>
				<th>Numero</th>
				<th>Total</th>
     
			</tr>
		</thead>
		<tbody>
			<% @order.each do |product| %>
				<tr>
					<td><%= product.id %></td>
					<td><%= product.subtotal %></td>
					
          
				</tr>
			<% end %>
		</tbody>
	</table>
	

</div>